
bool libapx_is_integer(string s) //Stolen from Cortex ???? hope it works ??? :)
{
	int x = 1;
	string c1 = "";
	while( x != StringLength(s) )
	{
		c1 = StringSub(s, x, x);
		if( !(c1 == "0" || c1 == "1" || c1 == "2" || c1 == "3" || c1 == "4" || c1 == "5" || c1 == "6" || c1 == "7" || c1 == "8" || c1 == "9") ) {
			return false;
		}
		x += 1;
	}
	return true;
}
